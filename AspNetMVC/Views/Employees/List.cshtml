@model List<Employee>

<ul>
    @foreach (var employee in Model)
    {
        <li>
            @employee.Id - <a asp-controller="Employees" asp-action="View" asp-route-id="@employee.Id">@employee.Name</a>
            (<a asp-controller="Employees" asp-action="Edit" asp-route-id="@employee.Id">Edit</a>)
        </li>
    }
</ul>
